import assert

as = assert.Assert

as(6, 4 + 2)
as(2, 4 - 2)
as(8, 4 * 2)
as(2, 4 / 2)
as(0, 4 % 2)
as(15, 4 + 2 * 3 + 5)
as(9, 4 + 2 / 3 + 5)
as(11, 4 + 2 % 3 + 5)
as(40, 5 * (3 + 5))
as(-10, -10)
as(-20, -10-10)
as(0, -10--10)
as(0, -10-+-10)
as(10.1, 0.1 + 10)
as(1.0, 0.1 * 10)
as(1.0, 0.1 * 10.0)
as(3, (1+2) * (3-2))
as(19, (1*2) + ((3-2) * 12 +(1*5)))

print("arith test succeeded")
