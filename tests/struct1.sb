import assert

as = assert.assert

struct person{
    name
    age

    def birthday() {
        age += 1
    }

    def setname(n) {
        name = n
    }

    def getname() {
        return name
    }
}

p = person{name: "alice", age: 3}
as("alice", p.name)
as(3, p.age)
p.birthday()
as(4, p.age)
as("alice", p.getname())
p.setname("bob")
as("bob", p.getname())
as(4, p.age)

print("struct1 test succeeded")
