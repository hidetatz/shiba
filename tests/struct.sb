import assert

as = assert.assert

struct person{
    name
    age

    def birthday() {
        age += 1
    }

    def setname(n) {
        name = n
    }

    def getname() {
        return name
    }
}

p = person{name: "alice", age: 3}
as("alice", p.name)
as(3, p.age)

print("struct test succeeded")
