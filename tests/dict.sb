import assert

as = assert.assert

d = {}
as({}, d)

d[1] = 2
as({1: 2}, d)
as(2, d[1])

d = {1: "a", 2: true, 3: [1, 2, 3], [1, 2, 3]: [4, 5, 6], 5: {"a": "b"}}
as("a", d[1])
as(true, d[2])
as([1, 2, 3], d[3])
as([4, 5, 6], d[[1, 2, 3]])
as([4, 5, 6], d[d[3]])
as("b", d[5]["a"])

d = {9: "a", 8: "b", 7: "c"}

for i, e in d {
    if i == 0 {
        as(9, e)
    } elif i == 1 {
        as(8, e)
    } else {
        as(7, e)
    }
}

print("dict test succeeded")
