import assert

as = assert.assert

as([1, 2], [1] + [2])
as([1, 1], [1] * 2)
as([1, true, "s", [4], 1, true, "s", [4]], [1, true, "s", [4]] * 2)
as(1, [1, 2, 3][0])
as(2, [1, 2, 3][1])
as(3, [1, 2, 3][2])
as([1, 2, 3], [1, 2, 3][0:3])
as([1, 2], [1, 2, 3][0:2])
as([1], [1, 2, 3][0:1])
as([], [1, 2, 3][0:0])
as([2], [1, 2, 3][1:2])
as([2, 3], [1, 2, 3][1:3])

print("list test succeeded")
